    "def instertion_sort(A):\n",
    "    for i in range(1,len(A)):\n",
    "        for j in range(i-1,-1,-1):\n",
    "            if A[j] > A[j+1]:\n",
    "                A[j], A[j+1] = A[j+1], A[j]\n",
    "            else:\n",
    "                break\n",
    "    return A"
   ]
